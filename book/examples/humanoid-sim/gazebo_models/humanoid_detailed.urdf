  </link>

  <joint name="left_hip" type="revolute">
    <parent link="torso"/>
    <child link="left_upper_leg"/>
    <origin xyz="0 0.15 -0.1" rpy="1.5707 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="25" velocity="1"/>
  </joint>

  <link name="left_lower_leg">
